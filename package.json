{
  "name": "simple-fullstack-app",
  "version": "1.0.0",
  "description": "Simple full-stack app",
  "main": "index.js",
  "scripts": {
    "start": "node server/index.js",
    "test": "istanbul cover _mocha ./server/test/**/*.test.js",
    "build:app": "ng build --prod",
    "db:createdb": "psql --host docker_postgres_db --user postgres --command \"CREATE DATABASE simple_fullstack_app;\" &>/dev/null",
    "db:loaddata": "node scripts/initializeDatabase.js",
    "start:nodemon": "nodemon server/index.js",
    "start:docker_debug": "sleep 3 && npm run db:createdb && sleep 1 && npm run db:loaddata && npm run start:nodemon"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manueldziuba/simple-fullstack-app.git"
  },
  "keywords": [
    "fullstack",
    "node",
    "postgres",
    "angular"
  ],
  "engines": {
    "node": ">=8"
  },
  "author": "Manuel Dziuba",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/manueldziuba/simple-fullstack-app/issues"
  },
  "homepage": "https://github.com/manueldziuba/simple-fullstack-app#readme",
